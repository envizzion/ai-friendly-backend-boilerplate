{
  "scripts": {
    "dev": "tsx watch src/index.ts | pino-pretty",
    "start": "node dist/index.js",
    "build": "tsc && tsc-alias",
    "lint": "eslint --ext .ts,.tsx src/",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:generate-core-types": "kysely-codegen --camel-case --out-file ./src/shared/database/schemas/core/kysely-types.ts --include-pattern 'manufacturer|model|part|user|file|color|body_style|vin_prefix|vehicle_hierarchy_audit|part_*|model_variation*'",
    "db:generate-vendor-types": "kysely-codegen --camel-case --out-file ./src/shared/database/schemas/vendor/kysely-types.ts --include-pattern 'vendor_*'",
    "db:generate-customer-types": "kysely-codegen --camel-case --out-file ./src/shared/database/schemas/customer/kysely-types.ts --include-pattern 'customer_*|shopping_*|wishlist*'",
    "db:generate-all-types": "npm run db:generate-core-types && npm run db:generate-vendor-types && npm run db:generate-customer-types",
    "schema:generate": "kysely-codegen --camel-case --out-file ./src/db/schema/generated.ts",
    "fix:imports": "node scripts/fix-imports.js",
    "prepare": "mkdir -p scripts && chmod +x scripts/fix-imports.js",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "openapi:yaml": "swagger-cli bundle ./openapi/openapi.json -o ./openapi/openapi.yaml -t yaml",
    "encode:credentials": "node scripts/encode-credentials.js"
  },
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-textract": "^3.830.0",
    "@google-cloud/storage": "^7.16.0",
    "@google-cloud/vision": "^5.2.0",
    "@google/genai": "^1.7.0",
    "@hono/node-server": "^1.13.7",
    "@hono/swagger-ui": "^0.5.0",
    "@hono/zod-openapi": "^0.19.2",
    "@hono/zod-validator": "^0.4.2",
    "@mistralai/mistralai": "^1.7.2",
    "bullmq": "^5.36.0",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.4",
    "hono": "^4.6.18",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.4.2",
    "kysely": "^0.27.6",
    "pg": "^8.14.0",
    "pino": "^9.6.0",
    "postgres": "^3.4.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@anatine/zod-mock": "^3.13.4",
    "@biomejs/biome": "1.9.4",
    "@faker-js/faker": "^8.4.1",
    "@types/node": "^22.10.10",
    "@types/pg": "^8.11.11",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitest/coverage-v8": "^1.4.0",
    "@vitest/ui": "^1.4.0",
    "drizzle-kit": "^0.30.2",
    "eslint": "^9.22.0",
    "eslint-plugin-import": "^2.31.0",
    "kysely-codegen": "^0.18.3",
    "pino-pretty": "^13.0.0",
    "prettier": "3.4.2",
    "swagger-cli": "^4.0.4",
    "tsc-alias": "^1.8.11",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^1.4.0"
  },
  "engines": {
    "node": ">=20.0.0 <24.0.0"
  },
  "packageManager": "pnpm@9.11.0"
}