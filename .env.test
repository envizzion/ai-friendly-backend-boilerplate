# Test Environment Configuration
NODE_ENV=test
LOG_LEVEL=error

# Test Database (using existing PostgreSQL container)
DB_HOST=localhost
DB_USER=root
DB_PASSWORD=password
DB_NAME=parts-db-new

# PostgreSQL
DATABASE_URL=postgres://root:password@localhost/parts-db-new
TEST_DATABASE_URL=postgres://root:password@localhost/parts-db-test

# Redis (optional for testing)
REDIS_URL=redis://localhost:6379

# Disable external services in tests
ENABLE_CLOUD_SERVICES=false
ENABLE_EMAIL=false

# Test JWT Secret
JWT_SECRET=test-secret-key-for-testing-only